<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="insightSettings" type="ReflectSoftware.Insight.ConfigurationHandler,ReflectSoftware.Insight"/>
  </configSections>

  <!-- Logging Configuration 
       To use a satellite configuration file, replace the following with 
       <insightSettings externalConfigSource="Insight.config" /> 
       Then make sure you update the Insight.config file property 'Copy to Output Directory' to 'Copy If Newer'.
  -->
  <insightSettings externalConfigSource="Insight.config" />

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
</configuration>